{
    "manifest_version": 3,
    "name": "QuillBot AI Detector (Prototype)",
    "version": "0.1.0",
    "description": "Prototype extension to detect likelihood that text was generated by AI.",
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
      }
    },
    "permissions": ["storage"],
    "icons": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  }